<Window x:Class="WpfBookCodeSample.CodeSample.RoutedEventSample.CustomRoutedEvent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfBookCodeSample.Model"
        mc:Ignorable="d"
        Title="CustomRoutedEvent" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid x:Name="gd_1" Margin="10" Background="AliceBlue" local:TimeButton.ReportTime="ReportTimeHandle">
        <Grid x:Name="gd_2" Margin="10" Background="AntiqueWhite" local:TimeButton.ReportTime="ReportTimeHandle">
            <Grid x:Name="gd_3" Margin="10" Background="Aqua" local:TimeButton.ReportTime="ReportTimeHandle">
                <StackPanel Margin="10" Background="Aquamarine" x:Name="stackPanel_1" local:TimeButton.ReportTime="ReportTimeHandle">
                    <ListBox x:Name="listBox"></ListBox>
                    <local:TimeButton x:Name="TimeButton"  Content="报时" Width="50" 
                                      local:TimeButton.ReportTime="ReportTimeHandle"></local:TimeButton>
                </StackPanel>
            </Grid>
        </Grid>
    </Grid>
</Window>
