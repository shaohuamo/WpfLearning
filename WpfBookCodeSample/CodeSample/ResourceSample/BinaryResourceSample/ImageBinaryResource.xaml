<Window x:Class="WpfBookCodeSample.CodeSample.ResourceSample.BinaryResourceSample.ImageBinaryResource"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfBookCodeSample.CodeSample.ResourceSample.BinaryResourceSample"
        mc:Ignorable="d"
        Title="ImageBinaryResource" Height="800" Width="800" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <Image x:Name="Image1" Source="pack://application:,,,/Resources/Images/tulip_farm.jpg" Stretch="None"></Image>
        <Image x:Name="Image2" Source="../../../Resources/Images/tulip_farm.jpg" Stretch="None"></Image>
        <Image x:Name="Image3" Stretch="None"></Image>
        <Image x:Name="Image4" Stretch="None"></Image>
    </StackPanel>
</Window>
