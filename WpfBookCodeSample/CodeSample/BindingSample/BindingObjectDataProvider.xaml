<Window x:Class="WpfBookCodeSample.CodeSample.BindingSample.BindingObjectDataProvider"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="BindingObjectDataProvider" Height="300" Width="400" WindowStartupLocation="CenterScreen">
    <StackPanel Background="LightBlue">
        <TextBox x:Name="textBoxArg1" Width="200" Margin="15"/>
        <TextBox x:Name="textBoxArg2" Width="200" Margin="15"/>
        <TextBox x:Name="textBoxResult" Width="200" Margin="15"/>
        <Button Content="OK" Margin="15" Click="Button_Click"/>
    </StackPanel>
</Window>
