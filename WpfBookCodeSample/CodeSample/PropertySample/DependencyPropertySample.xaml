<Window x:Class="WpfBookCodeSample.CodeSample.PropertySample.DependencyPropertySample"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfBookCodeSample.CodeSample.PropertySample"
        mc:Ignorable="d"
        Title="DependencyPropertySample" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <StackPanel>
        <TextBox x:Name="textBox1" BorderBrush="Black" Margin="10"/>
        <TextBox x:Name="textBox2" BorderBrush="Black" Margin="10"/>
        <Button Content="Button1" Margin="5" Click="Button1_Click"/>
        <Button Content="Button2" Margin="5" Click="Button2_Click"/>
    </StackPanel>
</Window>
